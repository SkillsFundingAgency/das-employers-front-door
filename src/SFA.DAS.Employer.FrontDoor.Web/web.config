<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<system.webServer>

		<httpProtocol>
			<customHeaders>
				<remove name="X-Powered-By" />
				<!--<remove name="Server" />-->
			</customHeaders>
		</httpProtocol>
		<security>
			<!-- Remove Server header-->
			<requestFiltering removeServerHeader ="true" />
		</security>
	</system.webServer>

</configuration>