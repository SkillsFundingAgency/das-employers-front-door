@{
    ViewData["Title"] = "Page not found";
    ViewData["Description"] = "We could not find this page. Check the web address is correct and try again.";
    @*ViewData[nameof(BreadCrumbViewModel)] = new BreadCrumbViewModel {Level = BreadCrumbLevel.Error};*@
}

<main class="govuk-main-wrapper " id="main-content">
    <div class="govuk-width-container govuk-!-padding-top-3">
        <div id="not-found-outer" class="govuk-grid-row">
            <div id="not-found-inner" class="govuk-grid-column-two-thirds">
                <h1 id="not-found-header" class="govuk-heading-xl">Page not found</h1>

                <p id="not-found-body-typed" class="govuk-body">If you typed the web address, check it is correct.</p>
                <p id="not-found-body-pasted" class="govuk-body">If you pasted the web address, check you copied the entire address.</p>
                <p id="not-found-contact-link" class="govuk-body">If the web address is correct or you selected a link or button, <a href="https://help.apprenticeships.education.gov.uk/hc/en-gb#contact-us" class="govuk-link govuk-link--no-visited-state">contact the Apprenticeship Service</a> to get help.</p>

            </div>
        </div>
    </div>
</main>
